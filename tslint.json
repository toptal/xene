{
  "extends": "tslint:latest",
  "rules": {
    "curly": false,
    "member-access": false,
    "no-default-export": true,
    "ordered-imports": [
      false
    ],
    "unified-signatures": false,
    "semicolon": [
      true,
      "never"
    ],
    "no-implicit-dependencies": false,
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "adjacent-overload-signatures": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "arrow-parens": [
      false,
      "ban-single-arg-parens"
    ],
    "space-before-function-paren": [
      true,
      {
        "asyncArrow": "always"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-console": [
      false
    ],
    "array-type": [
      true,
      "array"
    ],
    "completed-docs": [
      false,
      {
        "methods": {
          "privacies": [
            "public",
            "protected"
          ]
        },
        "properties": {
          "privacies": [
            "public",
            "protected"
          ]
        },
        "functions": {
          "visibilities": [
            "exported"
          ]
        }
      }
    ]
  }
}
